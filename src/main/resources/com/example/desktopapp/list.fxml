<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.example.desktopapp.List">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label
            GridPane.columnIndex="0" GridPane.rowIndex="0">
    </Label>
    <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="1">
        <columns>
            <TableColumn text="id" fx:id="id">
            </TableColumn>
            <TableColumn text="Name" fx:id="name">
            </TableColumn>
            <TableColumn text="Age" fx:id="age">
            </TableColumn>
            <TableColumn text="Phone" fx:id="phone">
            </TableColumn>


        </columns>
    </TableView>

    <Button text="Sil" onAction="#onDeleteButtonClick"/>
</GridPane>
